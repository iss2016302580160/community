<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>/mainpages/login.jsp</welcome-file>
  </welcome-file-list>
  <servlet-mapping>
    <servlet-name>AddCommunity.do</servlet-name>
    <url-pattern>/AddCommunity.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddCommunity.do</servlet-name>
    <servlet-class>com.java.servlets.AddCommunity</servlet-class>
  </servlet>


  <servlet>
    <servlet-name>mapages.do</servlet-name>
    <servlet-class>com.java.servlets.mainpage</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>mapages.do</servlet-name>
    <url-pattern>/mapages.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>IoServlet.do</servlet-name>
    <servlet-class>com.java.servlets.loServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>IoServlet.do</servlet-name>
    <url-pattern>/IoServlet.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>update.do</servlet-name>
    <servlet-class>com.java.servlets.Update</servlet-class>
  </servlet>
 <servlet-mapping>
   <servlet-name>update.do</servlet-name>
   <url-pattern>/update.do</url-pattern>
 </servlet-mapping>

</web-app>
